    "Database Row": 2.5,
    "Database Insert": 2,
    "Database Update": 2
  },
  "navigationLinks": {
    "Database Documentation": "https://dbdocs.io/rys68639ef0785a9/370-Binary-Bandits",
    "Live Deployment": "https://www.codeallergy.dev",
    "GitLab": "https://git.cs.usask.ca/fzl936/370-binary-bandits"
  },
  "favicon": "src/app/favicon.ico"
}