{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-zod", "typedoc-plugin-extras", "typedoc-plugin-merge-modules"],
  "entryPoints": ["./src"],
  "exclude": ["**/(examples)/**/*.{ts,tsx}", "src/components/**/*.{ts,tsx}",
    ".next"],
  "entryPointStrategy": "expand",
  "theme": "default",
  "categorizeByGroup": true,
  "hideGenerator": true,
  "tsconfig": "tsconfig.json",
